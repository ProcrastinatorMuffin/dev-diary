{
  "name": "dev-diary",
  "displayName": "DevDiary",
  "description": "Track. Reflect. Code.",
  "version": "0.0.2",
  "publisher": "procrastinatorMuffin",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.semantic_analysis"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.semantic_analysis",
        "title": "Semantic Analysis"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "function": [
            "function"
          ]
        },
        "legend": {
          "types": [
            "function"
          ]
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "minimatch": "3.1.2",
    "minimist": "1.2.8"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@npmcli/fs": "3.1.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@vscode/test-electron": "^2.3.4",
    "diff": "^4.0.2",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "minimatch": "^9.0.3",
    "minimist": "^1.2.8",
    "mocha": "^10.2.0",
    "sinon": "^15.2.0",
    "tough-cookie": "^4.1.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/node-fetch": "^2.6.4",
    "antlr4": "^4.13.0",
    "axios": "^1.4.0",
    "fetch": "^1.1.0",
    "mkdirp": "^3.0.1",
    "nan": "^2.17.0",
    "npm-force-resolutions": "^0.0.10",
    "sqlite3": "^5.1.6",
    "tree-sitter": "^0.20.5",
    "tree-sitter-cpp": "^0.20.3",
    "tree-sitter-java": "^0.19.1",
    "tree-sitter-javascript": "^0.19.0",
    "tree-sitter-python": "^0.20.3"
  }
}
